package energyconverter;

public interface EnergyService {
	
	public String jouleToCalorie(String energy);
	public String jouleToKilojoule(String energy);
	
	public String calorieTojoule(String energy);
	public String calorieToKilojoule(String energy);
	
	public String kilojouleToCalorie(String energy);
	public String kilojouleToJoule(String energy);

}
